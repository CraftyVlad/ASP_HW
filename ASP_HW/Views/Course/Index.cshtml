@model List<Course>

<h2>Courses</h2>

<ul>
    @foreach (var course in Model)
    {
        <li>
            <a href="/courses/details/@course.Id">@course.Name</a> | @course.StudentsEnrolled students |
            <a href="/courses/enroll/@course.Id">Enroll</a>
        </li>
    }
</ul>
<a href="/">Back to main</a>